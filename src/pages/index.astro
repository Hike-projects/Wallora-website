
---

import BaseHead from '../components/BaseHead.astro'; import Header from '../components/Header.astro'; import Footer from '../components/Footer.astro'; import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';

<!doctype html>

<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Header />
		<main>
			<h1>Wallora Wallpapers</h1>
			<p>We currently under maintenance. Please check back soon!</p>
			<h2>App Status</h2>
			<p>The Wallora Wallpapers app is under maintenance. We appreciate your patience.</p>
			<h2>Download the App</h2>
			<p>
				<a href="your-app-download-link" class="button">Download Wallora Wallpapers</a>
			</p>
			<h2>App Version</h2>
			<p>Current version: <strong>1.0.0</strong></p>
		</main>
		<Footer />
	</body>
</html>
